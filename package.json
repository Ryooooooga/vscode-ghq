{
  "name": "vscode-ghq",
  "displayName": "vscode-ghq",
  "description": "ghq interface for Visual Studio Code",
  "version": "0.2.0",
  "publisher": "marchrock",
  "repository": {
    "type": "git",
    "url": "https://github.com/marchrock/vscode-ghq"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.ghqOpen",
    "onCommand:extension.ghqOpenInNewWindow",
    "onCommand:extension.ghqGet"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.ghqOpen",
        "title": "Open Repository",
        "category": "GHQ"
      },
      {
        "command": "extension.ghqOpenInNewWindow",
        "title": "Open Repository in New Window",
        "category": "GHQ"
      },
      {
        "command": "extension.ghqGet",
        "title": "Get repository",
        "category": "GHQ"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "shelljs": "^0.7.5",
    "strip-ansi": "^3.0.1"
  }
}
